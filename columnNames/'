import sys

def letterInt(i):
    letter_dict = {0: 'Z'}
    for num in range(1,26):
        letter_dict.update({num: chr(num + 64)})
    print(letter_dict)
    
    ord_1 = i // 26**2
    ord_2 = i // (26**(ord_1)) 
    ord_3 = i % 26
    
    if i <= 26:
        return letter_dict[i]
    elif i <= 26**2:
        letter_2 = letter_dict[ord_2]
        letter_3 = letter_dict[ord_3]
	
        return ''.join([letter_2, letter_3])

print(letterInt(1))
print(letterInt(34))
